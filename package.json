{
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "coinbase-pro-node": "^3.0.5",
    "node-fetch": "^2.6.1",
    "rpc-websockets": "^7.4.12",
    "ws": "^7.5.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      },
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "extends": "eslint:all",
    "rules": {
      "no-console": 0,
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "id-length": 0,
      "no-mixed-requires": 0,
      "no-ternary": 0,
      "padded-blocks": 0,
      "space-before-function-paren": 0,
      "no-prototype-builtins": 0,
      "dot-location": 0,
      "no-magic-numbers": 0,
      "arrow-parens": 0,
      "object-curly-newline": 0,
      "no-extra-parens": 0,
      "one-var": 0,
      "capitalized-comments": 0,
      "no-undefined": 0,
      "sort-vars": 0,
      "new-cap": 0,
      "max-statements": 0,
      "func-style": 0,
      "arrow-body-style": 0,
      "require-jsdoc": 0,
      "array-bracket-newline": 0,
      "no-case-declarations": 0,
      "class-methods-use-this": 0,
      "no-labels": 0,
      "max-params": [
        "error",
        {
          "max": 5
        }
      ],
      "max-len": [
        "error",
        150
      ],
      "wrap-regex": 0,
      "require-await": 0,
      "array-element-newline": 0,
      "no-await-in-loop": 0,
      "consistent-this": [
        "error",
        "self"
      ],
      "no-confusing-arrow": 0,
      "sort-keys": 0,
      "max-lines": 0,
      "no-continue": 0,
      "multiline-comment-style": 0,
      "no-underscore-dangle": 0,
      "strict": 0,
      "function-call-argument-newline": 0,
      "no-promise-executor-return": 0,
      "max-lines-per-function": [
        "error",
        100
      ],
      "max-classes-per-file": 0,
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "ignore"
        }
      ],
      "prefer-named-capture-group": 0,
      "require-unicode-regexp": 0,
      "prefer-const": [
        "error",
        {
          "destructuring": "all"
        }
      ],
      "sort-imports": 0
    }
  },
  "devDependencies": {
    "ndb": "^1.1.5"
  }
}
